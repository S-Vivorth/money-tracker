<?xml version="1.0" encoding="utf-8"?>


<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >


    <item>
        <selector
            >
            <item android:state_pressed="false"
                >
                <shape
                    android:shape="rectangle"
                    >

                    <solid android:color="?attr/nextButtonBgColor"/>
                    <corners
                        android:radius="12dp"
                        />
                </shape>
            </item>
            <item android:state_pressed="true" >
                <shape
                    android:shape="rectangle"
                    >
                    <solid android:color="?attr/backBtnPressedBgColor"/>
                    <corners
                        android:radius="12dp"
                        />
                </shape>
            </item>
        </selector>
    </item>
</layer-list>