<?xml version="1.0" encoding="utf-8"?>


<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >


    <item>
        <selector
            >
            <item android:state_pressed="false"
                >
                <shape
                    android:shape="rectangle"
                    >

                    <solid android:color="?attr/quaternaryBgColor"/>
                    <stroke android:width="1.1dp" android:color="@color/light_black_6" />
                    <corners
                        android:radius="100dp"
                        />
                </shape>
            </item>
            <item android:state_pressed="true"
                >
                <shape
                    android:shape="rectangle"
                    >

                    <solid android:color="@color/light_black_6"/>
                    <stroke android:width="1dp" android:color="@color/light_black_6" />
                    <corners
                        android:radius="100dp"
                        />
                </shape>
            </item>
        </selector>
    </item>

</layer-list>