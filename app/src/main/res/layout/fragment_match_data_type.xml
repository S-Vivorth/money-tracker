<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/bgColor"
        tools:context="io.paraga.moneytrackerdev.views.bottomSheet.BottomSheetStartDateFragment">
        <include layout="@layout/menu_app_bar_bottomsheet"
            android:id="@+id/app_bar"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_ing"
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:layout_margin="12dp"
            android:background="@drawable/rounded_drawer_bottom_sheet"
            app:layout_constraintTop_toBottomOf="@+id/app_bar"
            >
            <ImageView
                android:id="@+id/img_ing"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/do_not_disturb_on"
                android:layout_marginStart="16dp"
                app:tint="?attr/primaryText"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                />
            <TextView
                android:id="@+id/text_ing"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/ingored"
                android:textSize="14sp"
                android:layout_marginStart="4dp"
                app:tint="?attr/iconClose"
                app:layout_constraintStart_toEndOf="@+id/img_ing"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                />

            <androidx.appcompat.widget.AppCompatCheckBox
                android:id="@+id/checkbox_ing"
                android:layout_width="40dp"
                android:layout_height="40dp"
                style="@style/CustomCheckbox"
                android:clickable="false"
                tools:checked="false"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_date"
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:layout_margin="12dp"
            android:background="@drawable/rounded_drawer_bottom_sheet"
            app:layout_constraintTop_toBottomOf="@+id/layout_ing"
            >
            <ImageView
                android:id="@+id/img_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/calendar_month"
                app:tint="?attr/primaryText"
                android:layout_marginStart="16dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                />
            <TextView
                android:id="@+id/text_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/date"
                android:textSize="14sp"
                app:tint="?attr/iconClose"
                android:layout_marginStart="4dp"
                app:layout_constraintStart_toEndOf="@+id/img_date"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                />
            <TextView
                android:id="@+id/row_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="#1"
                android:fontFamily="@font/inter_400"
                android:textSize="14sp"
                android:textColor="@color/lightDefaultIconTint"
                android:layout_marginEnd="10dp"
                android:visibility="gone"
                app:layout_constraintEnd_toStartOf="@+id/checkbox_date"
                app:layout_constraintTop_toTopOf="@+id/checkbox_date"
                app:layout_constraintBottom_toBottomOf="@+id/checkbox_date"/>
            <androidx.appcompat.widget.AppCompatCheckBox
                android:id="@+id/checkbox_date"
                android:layout_width="40dp"
                android:layout_height="40dp"
                style="@style/CustomCheckboxPink"
                android:clickable="false"
                tools:checked="false"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_remark"
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:layout_margin="12dp"
            android:background="@drawable/rounded_drawer_bottom_sheet"
            app:layout_constraintTop_toBottomOf="@+id/layout_date"
            >
            <ImageView
                android:id="@+id/img_remark"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/description"
                app:tint="?attr/primaryText"
                android:layout_marginStart="16dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                />
            <TextView
                android:id="@+id/text_remark"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/remark"
                android:textSize="14sp"
                app:tint="?attr/iconClose"
                android:layout_marginStart="4dp"
                app:layout_constraintStart_toEndOf="@+id/img_remark"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                />
            <TextView
                android:id="@+id/row_remark"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="#1"
                android:fontFamily="@font/inter_400"
                android:textSize="14sp"
                android:textColor="@color/lightDefaultIconTint"
                android:layout_marginEnd="10dp"
                android:visibility="gone"
                app:layout_constraintEnd_toStartOf="@+id/checkbox_remark"
                app:layout_constraintTop_toTopOf="@+id/checkbox_remark"
                app:layout_constraintBottom_toBottomOf="@+id/checkbox_remark"/>
            <androidx.appcompat.widget.AppCompatCheckBox
                android:id="@+id/checkbox_remark"
                android:layout_width="40dp"
                android:layout_height="40dp"
                style="@style/CustomCheckboxPink"
                android:clickable="false"
                tools:checked="false"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_wallet"
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:layout_margin="12dp"
            android:visibility="gone"
            android:background="@drawable/rounded_drawer_while"
            app:layout_constraintTop_toBottomOf="@+id/layout_remark"
            >
            <ImageView
                android:id="@+id/img_wallet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/account_balance_wallet"
                android:layout_marginStart="16dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                />
            <TextView
                android:id="@+id/text_wallet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/wallet"
                android:textSize="14sp"
                android:textColor="@color/black80"
                android:layout_marginStart="4dp"
                app:layout_constraintStart_toEndOf="@+id/img_wallet"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                />
            <TextView
                android:id="@+id/row_wallet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="#1"
                android:fontFamily="@font/inter_400"
                android:textSize="14sp"
                android:textColor="@color/lightDefaultIconTint"
                android:layout_marginEnd="10dp"
                android:visibility="gone"
                app:layout_constraintEnd_toStartOf="@+id/checkbox_wallet"
                app:layout_constraintTop_toTopOf="@+id/checkbox_wallet"
                app:layout_constraintBottom_toBottomOf="@+id/checkbox_wallet"/>
            <androidx.appcompat.widget.AppCompatCheckBox
                android:id="@+id/checkbox_wallet"
                android:layout_width="40dp"
                android:layout_height="40dp"
                style="@style/CustomCheckboxPink"
                android:clickable="false"
                tools:checked="false"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_category"
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:layout_margin="12dp"
            android:background="@drawable/rounded_drawer_bottom_sheet"
            app:layout_constraintTop_toBottomOf="@+id/layout_wallet"
            >
            <ImageView
                android:id="@+id/img_category"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/bookmarks"
                android:layout_marginStart="16dp"
                app:tint="?attr/primaryText"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                />
            <TextView
                android:id="@+id/text_category"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/category"
                android:textSize="14sp"
                app:tint="?attr/iconClose"
                android:layout_marginStart="4dp"
                app:layout_constraintStart_toEndOf="@+id/img_category"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                />
            <TextView
                android:id="@+id/row_category"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="#1"
                android:fontFamily="@font/inter_400"
                android:textSize="14sp"
                android:textColor="@color/lightDefaultIconTint"
                android:layout_marginEnd="10dp"
                android:visibility="gone"
                app:layout_constraintEnd_toStartOf="@+id/checkbox_category"
                app:layout_constraintTop_toTopOf="@+id/checkbox_category"
                app:layout_constraintBottom_toBottomOf="@+id/checkbox_category"/>
            <androidx.appcompat.widget.AppCompatCheckBox
                android:id="@+id/checkbox_category"
                android:layout_width="40dp"
                android:layout_height="40dp"
                style="@style/CustomCheckboxPink"
                android:clickable="false"
                tools:checked="false"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_amount"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="12dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@drawable/rounded_drawer_bottom_sheet"
            app:layout_constraintTop_toBottomOf="@+id/layout_category"
            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout_amount_auto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent">
                <ImageView
                    android:id="@+id/img_hrd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/hdr_auto"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
                <TextView
                    android:id="@+id/text_amount_auto"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/amount_auto"
                    app:tint="?attr/iconClose"
                    android:fontFamily="@font/inter_400"
                    android:textSize="14sp"
                    android:letterSpacing="0.05"
                    app:layout_constraintTop_toTopOf="@+id/img_hrd"
                    app:layout_constraintBottom_toBottomOf="@+id/img_hrd"
                    app:layout_constraintStart_toEndOf="@+id/img_hrd"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/system_will_auto_recognized_transaction_type_by_sign"
                    android:fontFamily="@font/inter_400"
                    android:textSize="11sp"
                    app:tint="?attr/arrowForwardIconColor"
                    app:layout_constraintStart_toStartOf="@+id/img_hrd"
                    app:layout_constraintTop_toBottomOf="@+id/img_hrd"/>
                <TextView
                    android:id="@+id/row_amount_auto"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="#1"
                    android:fontFamily="@font/inter_400"
                    android:textSize="14sp"
                    android:textColor="@color/lightDefaultIconTint"
                    android:layout_marginEnd="10dp"
                    android:visibility="gone"
                    app:layout_constraintEnd_toStartOf="@+id/checkbox_amount_auto"
                    app:layout_constraintTop_toTopOf="@+id/checkbox_amount_auto"
                    app:layout_constraintBottom_toBottomOf="@+id/checkbox_amount_auto"/>
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/checkbox_amount_auto"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    style="@style/CustomCheckboxPink"
                    android:clickable="false"
                    tools:checked="false"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                android:id="@+id/view_amount_auto"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="?attr/separatorColor"
                android:layout_marginTop="16dp"
                android:layout_marginHorizontal="16dp"
                app:layout_constraintTop_toBottomOf="@+id/layout_amount_auto"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout_amount_expense"
                android:layout_width="match_parent"
                android:layout_height="52dp"
                android:layout_marginTop="12dp"
                app:layout_constraintTop_toBottomOf="@+id/view_amount_auto"
                >
                <ImageView
                    android:id="@+id/img_amount_expense"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/arrow_circle_up"
                    android:layout_marginStart="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    />
                <TextView
                    android:id="@+id/text_amount_expense"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/amount_expense"
                    android:textSize="14sp"
                    app:tint="?attr/iconClose"
                    android:layout_marginStart="4dp"
                    app:layout_constraintStart_toEndOf="@+id/img_amount_expense"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    />
                <TextView
                    android:id="@+id/row_amount_expense"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="#1"
                    android:fontFamily="@font/inter_400"
                    android:textSize="14sp"
                    android:textColor="@color/lightDefaultIconTint"
                    android:layout_marginEnd="10dp"
                    android:visibility="gone"
                    app:layout_constraintEnd_toStartOf="@+id/checkbox_amount_expense"
                    app:layout_constraintTop_toTopOf="@+id/checkbox_amount_expense"
                    app:layout_constraintBottom_toBottomOf="@+id/checkbox_amount_expense"/>
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/checkbox_amount_expense"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    style="@style/CustomCheckboxPink"
                    android:clickable="false"
                    tools:checked="false"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                android:id="@+id/view_amount_expense"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="?attr/separatorColor"
                android:layout_marginTop="16dp"
                android:layout_marginHorizontal="16dp"
                app:layout_constraintTop_toBottomOf="@+id/layout_amount_expense"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout_amount_income"
                android:layout_width="match_parent"
                android:layout_height="52dp"
                android:layout_marginTop="12dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view_amount_expense"
                >
                <ImageView
                    android:id="@+id/img_amount_income"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/arrow_circle_down"
                    android:layout_marginStart="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    />
                <TextView
                    android:id="@+id/text_amount_income"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/amount_income"
                    android:textSize="14sp"
                    app:tint="?attr/iconClose"
                    android:layout_marginStart="4dp"
                    app:layout_constraintStart_toEndOf="@+id/img_amount_income"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    />
                <TextView
                    android:id="@+id/row_amount_income"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="#1"
                    android:fontFamily="@font/inter_400"
                    android:textSize="14sp"
                    android:textColor="@color/lightDefaultIconTint"
                    android:layout_marginEnd="10dp"
                    android:visibility="gone"
                    app:layout_constraintEnd_toStartOf="@+id/checkbox_amount_income"
                    app:layout_constraintTop_toTopOf="@+id/checkbox_amount_income"
                    app:layout_constraintBottom_toBottomOf="@+id/checkbox_amount_income"/>
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/checkbox_amount_income"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    style="@style/CustomCheckboxPink"
                    android:clickable="false"
                    tools:checked="false"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>